isbn = {10526234},
issn = {1052-6234},
journal = {SIAM Journal on Optimization},
keywords = {Clarke derivatives,constrained optimization,contingent cone,convergence analysis,hypertangent,mesh adaptive direct search algorithms (MADS),nonsmooth analysis},
number = {1},
pages = {188--217},
title = {{Mesh Adaptive Direct Search Algorithms for Constrained Optimization}},
url = {http://epubs.siam.org/doi/abs/10.1137/040603371},
volume = {17},
year = {2006}
}
