keywords = {Algorithms,Bayes Theorem,Computational Biology,Computational Biology: methods,Metabolic Engineering,Proteomics,Software,Systems Biology,Systems Biology: methods},
pages = {136},
pmid = {24885957},
title = {{MEIGO: An Open-Source Software Suite Based on Metaheuristics for Global Optimization in Systems Biology and Bioinformatics}},
