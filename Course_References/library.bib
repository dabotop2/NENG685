file = {:C$\backslash$:/Users/James/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kauffman et al. - 2013 - Alternative functional in vitro models of human intestinal epithelia.pdf:pdf},
isbn = {1663-9812 (Electronic)},
issn = {16639812},
keywords = {Human intestinal epithelial cell (hinEpC),Induced pluripotent stem cell (iPSC),Neonatal Fc receptor (FcRn),Transepithelial electrical resistance (TEER),permeability},
pmid = {23847534},
title = {{Alternative functional in vitro models of human intestinal epithelia}},
